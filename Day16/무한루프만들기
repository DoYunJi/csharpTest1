using System;

using System.Collections.Generic;

using System.Linq;

using System.Text;

using System.Threading.Tasks;



namespace MyApp101

{

    internal class Program

    {

        static void Main(string[] args)

        {

            while(true)

            {

                Console.Write("입력:");

                string str = Console.ReadLine();



                if(str == "q")

                {

                    Console.WriteLine("프로그램이 종료되었습니다. ");

                }

            }

            

        }

    }

}


